configuration {
	modi: "drun,ssh,emoji";
	fixed-num-lines: true;
	font: "Pragmata Pro {{ .rofi.font.size }}";
	location: 0;
	show-icons: false;
	terminal: "terminator";
	ssh-command: "{terminal} -x "{ssh-client} {host}"";
	run-command: "{cmd}";
	run-shell-command: "{terminal} -c "{cmd}"";
	matching: "fuzzy";
}

@import "gruvbox.rasi"
